{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "cd front_end && npm install && npm run build",
  "outputDirectory": "front_end/dist",
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@3.11.0"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" }
  ]
}
